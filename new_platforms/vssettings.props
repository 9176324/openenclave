    <ExternalDir>$(OESdkDir)3rdparty\</ExternalDir>
    <OpteeDir>$(ExternalDir)optee_os\</OpteeDir>
    <ProjectKaylaDir>$(ExternalDir)project-kayla\</ProjectKaylaDir>
    <RIoTDir>$(ExternalDir)RIoT\</RIoTDir>
    <TinyCborDir>$(RIoTDir)External\tinycbor\</TinyCborDir>
    <GTestDir>$(ExternalDir)googletest\googletest\</GTestDir>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(NewPlatformsDir)include;$(TinyCborDir)Src;$(RIoTDir)CyReP\cyrep;$(RIoTDir)CyReP\tcps;%(AdditionalIncludeDirectories);$(MSBuildThisFileDirectory)</AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>
